{
    "name": "neatlife/pframework",
    "description": "a modern framework for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "suxiaolin",
            "email": "dear.lin@live.com"
        }
    ],
    "require": {
        "monolog/monolog": "^1.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-foundation": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "pframework\\": "src/pframework/",
            "app\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
